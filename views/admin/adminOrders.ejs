  <style>
    body {
      background-color: #f8f9fa;
      font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
    }
    
    .navbar {
      background-color: #2c3e50;
      color: white;
      padding: 12px 20px;
    }
    
    .navbar-brand {
      display: flex;
      align-items: center;
      gap: 10px;
      color: white;
      text-decoration: none;
    }
    
    .container {
      max-width: 1200px;
      margin: 0 auto;
      padding: 20px;
    }
    
    .header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-bottom: 20px;
    }
    
    .search-input {
      background-color: #f1f1f1;
      border: none;
      border-radius: 4px;
      padding: 8px 12px 8px 30px;
      width: 220px;
    }
    
    .search-container {
      position: relative;
    }
    
    .search-icon {
      position: absolute;
      left: 10px;
      top: 50%;
      transform: translateY(-50%);
      color: #6c757d;
    }
    
    .table {
      background-color: white;
      margin-bottom: 0;
    }
    
    .table th {
      font-weight: 500;
      padding: 12px 16px;
      border-top: none;
      border-bottom: 1px solid #dee2e6;
      color: #495057;
    }
    
    .table td {
      padding: 16px;
      vertical-align: middle;
      border-bottom: 1px solid #dee2e6;
    }
    
    .product-image {
      width: 32px;
      height: 32px;
      border-radius: 50%;
      background-color: #f0f0f0;
      display: flex;
      align-items: center;
      justify-content: center;
      margin-right: 12px;
      overflow: hidden;
    }
    
    .product-item {
      display: flex;
      align-items: center;
    }
    
    .status-badge {
      display: inline-flex;
      padding: 6px 12px;
      border-radius: 4px;
      font-size: 14px;
      font-weight: normal;
    }
    
    .status-processing {
      background-color: #fff8e6;
      color: #856404;
    }
    
    .status-shipped {
      background-color: #e6f7ff;
      color: #0062cc;
    }
    
    .status-delivered {
      background-color: #e6fff1;
      color: #155724;
    }
    
    .status-cancelled {
      background-color: #ffe6e6;
      color: #721c24;
    }
    
    .status-container {
      display: flex;
      align-items: center;
    }
    
    .dropdown-icon {
      margin-left: 6px;
    }
    
    .update-btn {
      background-color: #dc3545;
      color: white;
      border: none;
      border-radius: 4px;
      padding: 6px 12px;
      font-size: 14px;
    }
    
    .pagination-container {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 16px 0;
    }
    
    .pagination {
      margin-bottom: 0;
    }
    
    .page-item.active .page-link {
      background-color: #2c3e50;
      border-color: #2c3e50;
    }
    
    .page-link {
      color: #2c3e50;
    }
    
    .page-info {
      color: #6c757d;
      font-size: 14px;
    }
    
    .sort-by-dropdown {
      display: inline-flex;
      align-items: center;
      gap: 5px;
      padding: 8px 12px;
      background-color: white;
      border: 1px solid #ced4da;
      border-radius: 4px;
      cursor: pointer;
      font-size: 14px;
    }
    
    .filter-dropdown {
      display: inline-flex;
      align-items: center;
      gap: 5px;
      padding: 8px 12px;
      margin-left: 10px;
      background-color: white;
      border: 1px solid #ced4da;
      border-radius: 4px;
      cursor: pointer;
      font-size: 14px;
    }
    
    .filter-icon {
      color: #6c757d;
    }
    .body-divv {
    width: 100%;
    margin-left: 190px;
}
